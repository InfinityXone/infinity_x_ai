    "scripts":  {
                    "frontend":  "vite",
                    "builder":  "ts-node --esm src/builder/builder-cli.ts",
                    "backend":  "ts-node --esm backend/server.ts",
                    "infinity-win":  "powershell -Command \"Start-Process powershell -ArgumentList \u0027-NoExit\u0027, \u0027-Command\u0027, \u0027cd C:\\Users\\Neoge\\Documents\\infinity_x_ai; pnpm run backend\u0027; Start-Sleep -Seconds 2; pnpm run frontend\"",
                    "infinity":  "start /B cmd /c \"pnpm run backend\" \u0026\u0026 pnpm run frontend",
                    "build":  "echo \u0027Using pre-built server.cjs\u0027",
                    "start":  "node server.cjs",
                    "dev":  "ts-node --esm src/index.ts",
